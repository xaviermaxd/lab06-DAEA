<Window x:Class="Lab05.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lab05"
        mc:Ignorable="d"
        Title="MainWindow" Height="250" Width="300">
    <Grid>
        <StackPanel>
            <Button Content="Ingresar Empleado" Click="Button_Click_Insert" FontSize="15" HorizontalAlignment="Center" Margin="0,20,0,0"/>
            <Button Content="Mostrar Lista De Empleados" Click="Button_Click_Select" FontSize="15" HorizontalAlignment="Center" Margin="0,20,0,0" Height="28"/>
        </StackPanel>
    </Grid>
</Window>
